<% attributes.each do |attribute|
	 if attribute.is_a? Hash then %>
		<%=	f.input attribute[:name].to_sym, as: :fake, required: attribute[:required], input_html: {
				name: "cloud_component[config][#{attribute[:name]}",
				value: component.config[attribute[:name]]
		} %>
	<% else %>
		<%= f.input attribute.to_sym, as: :fake, input_html: {
			name: "cloud_component[config][#{attribute}]",
			value: component.config[attribute]
		} %>
	<% end %>
<% end %>
